<odoo>

    <record id="view_settings_custom_tree" model="ir.ui.view">
        <field name="name">settings.custom.tree</field>
        <field name="model">settings.custom</field>
        <field name="arch" type="xml">
            <tree>
                <field name="affectation_multiple" string="Type d'affectation"/>
                <field name="id" string="ID"/>
            </tree>
        </field>
    </record>

    <record id="view_settings_custom_from" model="ir.ui.view">
        <field name="name">settings.custom.form</field>
        <field name="model">settings.custom</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="affectation_multiple" string="Type d'affectation"/>

                </group>
            </form>
        </field>
    </record>

    <record id="action_settings_custom" model="ir.actions.act_window">
        <field name="name">Settings custom</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">settings.custom</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{}</field>
        <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('settings_custom.view_settings_custom_tree')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('settings_custom.view_settings_custom_from')})]"/>
    </record>

    <menuitem id="menu_settings_custom" name="Table ParamÃ¨tres" parent="project_custom.menu_conf" sequence="10"
              action="action_settings_custom"/>

