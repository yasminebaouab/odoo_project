<odoo>

    <record id="project_type_view_tree" model="ir.ui.view">
        <field name="name">project.type.tree</field>
        <field name="model">project.type.custom</field>
        <field name="arch" type="xml">
            <tree string="Type Projet" editable="bottom">
                <field name="name"/>
                <field name="id"/>
            </tree>
        </field>
    </record>

    <record id="action_project_type" model="ir.actions.act_window">
        <field name="name">Types Projets</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">project.type.custom</field>
        <field name="view_mode">tree</field>
        <field name="context">{}</field>
    </record>

</odoo>